<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sg.mobilerecharge"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.sg.Activity.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logoo"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity android:name="com.sg.Activity.ReportOptionsActivity" >
        </activity>
        <activity android:name="com.sg.Activity.RevertBalanceActivity" >
        </activity>
        <activity android:name="com.sg.Activity.DashBoardActivity" >
        </activity>
        <activity android:name="com.sg.Activity.MobileRecharge" >
        </activity>
        <activity android:name="com.sg.Activity.DthRecharge" >
        </activity>
        <activity android:name="com.sg.Activity.UtilityRecharge" >
        </activity>
        <activity android:name="com.sg.Activity.OperatorListtActivity" >
        </activity>
        <activity android:name="com.sg.Activity.ComplaintActivity" >
        </activity>
        <activity android:name="com.sg.Activity.DthBooking" >
        </activity>
        <activity android:name="com.sg.Activity.WalletTransferActivity" >
        </activity>
        <activity
            android:name="com.sg.Activity.Dialogactivity"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity android:name="com.sg.Activity.ChangePassword" />
        <activity android:name="com.sg.Activity.ReportList" />
        <activity android:name="com.sg.Activity.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name="com.sg.localBroadcast.MyLocalBroadcast"
            android:enabled="true" >
            <intent-filter>
                <action android:name="com.sg.localBroadcast.MyLocalBroadcast" />
            </intent-filter>
        </receiver>
    </application>

</manifest>